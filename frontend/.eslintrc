{
  "extends": ["wesbos"],
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "jsx-a11y/tabindex-no-positive": 0,
    "react-hooks/exhaustive-deps": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
